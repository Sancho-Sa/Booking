<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="bookingPU" transaction-type="JTA">
        <jta-data-source>jdbc/booking</jta-data-source>
        <class>edu.booking.booking.entity.Privilege</class>
        <class>edu.booking.booking.entity.User</class>
        <class>edu.booking.booking.entity.Country</class>
        <class>edu.booking.booking.entity.Town</class>
        <class>edu.booking.booking.entity.Airport</class>
        <class>edu.booking.booking.entity.Clazz</class>
        <class>edu.booking.booking.entity.Passengers</class>
        <class>edu.booking.booking.entity.Flight</class>
        <class>edu.booking.booking.entity.Cost</class>
    </persistence-unit>
</persistence>
